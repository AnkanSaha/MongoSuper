{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended"],
  "jsRules": {
    "no-console": true,
    "no-debugger": true,
    "no-var-keyword": true,
    "no-duplicate-variable": true,
    "no-require-imports": true,
    "no-unused-expression": true,
    "no-reference": true,
    "no-construct": true,
    "no-eval": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": true,
    "prefer-const": true,
    "radix": true,
    "semicolon": true
  },
  "rules": {
    "no-console": false,
    "no-debugger": true,
    "no-var-keyword": true,
    "no-duplicate-variable": true,
    "no-require-imports": true,
    "no-unused-expression": true,
    "no-internal-module": true,
    "no-namespace": true,
    "no-reference": true,
    "no-construct": true,
    "no-eval": true,
    "no-string-literal": true,
    "no-switch-case-fall-through": true,
    "no-trailing-whitespace": true,
    "no-var-requires": true,
    "prefer-const": true,
    "radix": true,
    "semicolon": true
  },
  "rulesDirectory": []
}
